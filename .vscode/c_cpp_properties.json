{
    "configurations": [
        {
            "name": "Pro (OSX)",
            "includePath": [
                "${workspaceFolder}/srcCpp",
                "${workspaceFolder}/srcCppCommon",
                "${NDDSHOME}/include",
                "${NDDSHOME}/include/ndds"
            ],
            "defines": [
                "RTI_MICRO=0",
                "RTI_SECURE_PERFTEST=1"
            ],
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64"
        },
        {
            "name": "Micro (OSX)",
            "includePath": [
                "${workspaceFolder}/srcCpp",
                "${workspaceFolder}/srcCppCommon",
                "${RTIMEHOME}/include",
                "${RTIMEHOME}/include/rti_me",
                "${RTIMEHOME}/include/rti_me/dds_c",
                "${RTIMEHOME}/include/rti_me/dds_cpp"
            ],
            "defines": [
                "RTI_MICRO=1",
                "RTI_UNIX=1",
                "RTI_LANGUAGE_CPP_TRADITIONAL=1"
            ],
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64"
        }
    ],
    "version": 4
}